[Unit]
Description=Set Bluetooth MAC Address (%I)
Requires=me176c-factory.service
After=me176c-factory.service

[Service]
Type=oneshot
EnvironmentFile=@RUNTIME_DIRECTORY@/environment
ExecStart=/usr/bin/btmgmt -i %i public-addr $BDADDR

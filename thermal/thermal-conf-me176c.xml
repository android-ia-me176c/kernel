<!--
  Note: These values are based on similar trip points used on the
  stock (Android) system of the tablet. They begin throttling quite early
  based on the device (not CPU) sensors.

  Feel free to contribute if you think these values can be improved.
-->
<ThermalConfiguration>
<Platform>
    <Name>ME176C</Name>
    <ProductName>ME176C</ProductName>

    <ThermalZones>
        <ThermalZone>
            <Type>CPU</Type>
            <TripPoints>
                <TripPoint>
                    <SensorType>TCPU</SensorType>
                    <Temperature>95000</Temperature>
                    <type>passive</type>
                    <ControlType>SEQUENTIAL</ControlType>

                    <CoolingDevice>
                        <index>1</index>
                        <type>rapl_controller</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>2</index>
                        <type>intel_pstate</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>3</index>
                        <type>intel_powerclamp</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>4</index>
                        <type>Processor</type>
                    </CoolingDevice>
                </TripPoint>
            </TripPoints>
        </ThermalZone>

        <ThermalZone>
            <Type>SoC0</Type>
            <TripPoints>
                <TripPoint>
                    <SensorType>soc_dts0</SensorType>
                    <Temperature>90000</Temperature>
                    <type>passive</type>
                    <ControlType>SEQUENTIAL</ControlType>

                    <CoolingDevice>
                        <index>1</index>
                        <type>rapl_controller</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>2</index>
                        <type>intel_pstate</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>3</index>
                        <type>intel_powerclamp</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>4</index>
                        <type>Processor</type>
                    </CoolingDevice>
                </TripPoint>

                <TripPoint>
                    <SensorType>soc_dts0</SensorType>
                    <Temperature>100000</Temperature>
                    <type>critical</type>
                </TripPoint>
            </TripPoints>
        </ThermalZone>

        <ThermalZone>
            <Type>SoC1</Type>
            <TripPoints>
                <TripPoint>
                    <SensorType>soc_dts1</SensorType>
                    <Temperature>90000</Temperature>
                    <type>passive</type>
                    <ControlType>SEQUENTIAL</ControlType>

                    <CoolingDevice>
                        <index>1</index>
                        <type>rapl_controller</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>2</index>
                        <type>intel_pstate</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>3</index>
                        <type>intel_powerclamp</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>4</index>
                        <type>Processor</type>
                    </CoolingDevice>
                </TripPoint>

                <TripPoint>
                    <SensorType>soc_dts1</SensorType>
                    <Temperature>100000</Temperature>
                    <type>critical</type>
                </TripPoint>
            </TripPoints>
        </ThermalZone>

        <ThermalZone>
            <Type>STR1</Type>
            <TripPoints>
                <TripPoint>
                    <SensorType>STR1</SensorType>
                    <Temperature>55000</Temperature>
                    <type>passive</type>
                    <ControlType>SEQUENTIAL</ControlType>

                    <CoolingDevice>
                        <index>1</index>
                        <type>rapl_controller</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>2</index>
                        <type>intel_pstate</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>3</index>
                        <type>intel_powerclamp</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>4</index>
                        <type>Processor</type>
                    </CoolingDevice>
                </TripPoint>

                <TripPoint>
                    <SensorType>STR1</SensorType>
                    <Temperature>64000</Temperature>
                    <type>critical</type>
                </TripPoint>
            </TripPoints>
        </ThermalZone>

        <ThermalZone>
            <Type>STR2</Type>
            <TripPoints>
                <TripPoint>
                    <SensorType>STR2</SensorType>
                    <Temperature>55000</Temperature>
                    <type>passive</type>
                    <ControlType>SEQUENTIAL</ControlType>

                    <CoolingDevice>
                        <index>1</index>
                        <type>rapl_controller</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>2</index>
                        <type>intel_pstate</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>3</index>
                        <type>intel_powerclamp</type>
                    </CoolingDevice>
                    <CoolingDevice>
                        <index>4</index>
                        <type>Processor</type>
                    </CoolingDevice>
                </TripPoint>

                <TripPoint>
                    <SensorType>STR2</SensorType>
                    <Temperature>64000</Temperature>
                    <type>critical</type>
                </TripPoint>
            </TripPoints>
        </ThermalZone>
    </ThermalZones>

</Platform>
</ThermalConfiguration>
